{
  "compilerOptions": {
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "noUnusedLocals": false,
    "removeComments": false,
    "sourceMap": false,
    "target": "es2015",
    "module": "es2015",
    "jsx": "react",
    "moduleResolution": "node",
    "alwaysStrict": true,
    "declaration": true,
    "declarationDir": "../@types/react-power-table/",
    "stripInternal": true,
    "outDir": "../dist"
  },
  "include": [
    "./index.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "awesomeTypescriptLoaderOptions": {
    "silent": true
  }
}