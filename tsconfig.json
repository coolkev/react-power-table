{
  "compilerOptions": {
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "noUnusedLocals": false,
    "removeComments": false,
    "sourceMap": true,
    "target": "es2015",
    "module": "es2015",
    "jsx": "react",
    "moduleResolution": "node",
    "alwaysStrict": true,
    "allowJs": false,
    "declaration": false,
    "stripInternal": true,
    "allowSyntheticDefaultImports": true
  }
}