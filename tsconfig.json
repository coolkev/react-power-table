{
  "compilerOptions": {
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "noUnusedLocals": false,
    "noStrictGenericChecks": false,
    "removeComments": false,
    //"sourceMap": true,
    "target": "esnext",
    //"module": "commonjs",
    "jsx": "react",
    "noEmit": true,
    "moduleResolution": "node",
    "alwaysStrict": true,
    "declaration": false,
    "stripInternal": true,
    "esModuleInterop": true,
    "isolatedModules": false
  },
  "include": ["src"]
}