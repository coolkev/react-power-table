{
  "compilerOptions": {
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedParameters": false,
    "noUnusedLocals": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es2015",
    "module": "es2015",
    "jsx": "react",
    "alwaysStrict": true,
    "noEmitOnError": true,
    "moduleResolution": "node",
    "declaration": false,
    "suppressImplicitAnyIndexErrors": true
  },
  "include": [
    "src/app.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}